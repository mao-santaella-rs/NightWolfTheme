      {
        "label": "Night Wolf (dark gray) (No Italics)",
        "uiTheme": "vs-dark",
        "path": "./themes/night-wolf-dark-gray-noitalics.json"
      },
      {
        "label": "Night Wolf (gray)",
        "uiTheme": "vs-dark",
        "path": "./themes/night-wolf-gray.json"
      },
      {
        "label": "Night Wolf (gray) (No Italics)",
        "uiTheme": "vs-dark",
        "path": "./themes/night-wolf-gray-noitalics.json"
